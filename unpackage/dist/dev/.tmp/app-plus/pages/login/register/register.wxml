<view><view class="loginText">欢迎注册</view><view class="register"><navigator style="display:inline-block;" url="../login">已有账号，立即登录</navigator></view><form action><input class="phoneInput loginInput" maxlength="11" type="number" value="{{phone}}" placeholder="请输入手机号码" placeholder-class="pla" data-event-opts="{{[['input',[['__set_model',['','phone','$event',[]]]]]]}}" bindinput="__e"/><view class="yzmLogin"><input class="yzmInput" type="text" value="{{yzm}}" placeholder="请输入验证码" placeholder-class="pla" data-event-opts="{{[['input',[['__set_model',['','yzm','$event',[]]]]]]}}" bindinput="__e"/><block wx:if="{{second==''}}"><text data-event-opts="{{[['tap',[['yzmBtn',['$event']]]]]}}" class="yzmBtn" bindtap="__e">获取验证码</text></block><block wx:else><text class="yzmTime">{{second+"s"}}</text></block></view><input class="passwordInput loginInput" type="password" value="{{password}}" placeholder="请输入登录密码(6-18位字符)" placeholder-class="pla" maxlength="16" data-event-opts="{{[['input',[['__set_model',['','password','$event',[]]]]]]}}" bindinput="__e"/><input class="passwordInput loginInput" type="password" value="{{rePassword}}" placeholder="请再次输入登录密码（6-18位字符）" placeholder-class="pla" data-event-opts="{{[['input',[['__set_model',['','rePassword','$event',[]]]]]]}}" bindinput="__e"/><button class="{{[phone==''||password==''||yzm==''||rePassword==''?'loginBtn loginnBtNo':'loginBtn loginnBtnYes']}}" type="submit" disabled="{{phone==''||password==''||yzm==''||rePassword==''?true:false}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">注册</button></form><view><view class="orderLoginView"><text>第三方登录</text></view><view class="orderLoginBtn"><image class="qqlogin" src="../../../static/login/qq.png" mode="widthFix"></image><image class="weixinlogin" src="../../../static/login/weixin.png" mode="widthFix"></image></view></view></view>