<view><view class="Signature"><view class="nav"><image class="navlogo" src="../../../static/studySquare/Signaturelogo.png"></image><view class="navinfouser"><text class="username">{{userinfo.nickname}}</text><image class="usertoux" src="{{userinfo.avatar}}"></image></view></view><view class="banner"><swiper class="swiper" indicator-dots="true" autoplay="true" interval="5000" duration="1000"><block wx:for="{{bannerUrl}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><swiper-item><image style="width:100%;height:300rpx;" src="{{item.pic_url}}" mode="aspectFill" data-type="{{item.jump_type}}" data-url="{{item.to_url}}" data-event-opts="{{[['tap',[['jump',['$event']]]]]}}" bindtap="__e"></image></swiper-item></block></block></swiper></view><form report-submit="true" data-event-opts="{{[['submit',[['submitInfo',['$event']]]]]}}" bindsubmit="__e"><view class="formcont"><view class="section"><text class="hint">姓名：</text><input class="input" placeholder-class="place-hoder" type="text" placeholder="请输入您的名字" data-event-opts="{{[['input',[['__set_model',['','name','$event',[]]],['getName',['$event']]]]]}}" value="{{name}}" bindinput="__e"/></view><view class="section section2"><text class="hint">手机号：</text><input class="input" placeholder-class="place-hoder" type="number" placeholder="请输入您的手机号" data-event-opts="{{[['input',[['__set_model',['','mobile','$event',[]]],['getMobile',['$event']]]]]}}" value="{{mobile}}" bindinput="__e"/></view><view class="section section2 form-item datedirsday"><text class="hint borthday">生日:</text><block wx:if="{{birthtime_select==true}}"><picker class="borth-input" mode="date" value="birthtime" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{birthtime}}<image class="xaiicon" src="/static/img/my/righticon.png"></image></view></picker></block><block wx:if="{{birthtime_select==false}}"><picker class="borth-inputs" mode="date" value="birthtime" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><view class="picker">{{birthtime}}<image class="xaiicon" src="/static/img/my/righticon.png"></image></view></picker></block></view><view class="sectionbtn"><button class="submitbtn" type="submit" form-type="submit" data-event-opts="{{[['tap',[['signUpload',['$event']]]]]}}" bindtap="__e">提交</button></view></view></form></view><view class="rightfiex"><button class="rightfiex_cont rightfiex_first" open-type="share"><image class="icon addhao kefu" src="/static/img/freeTeaching/addhao.png"></image></button></view></view>