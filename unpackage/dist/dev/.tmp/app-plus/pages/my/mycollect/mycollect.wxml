view class="tabcont" hidden="{{currentId!=2}}"><view class="tabcontlist"><block wx:for="{{tabcontitem}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="tabcontitem" data-vp_id="{{item.vp_id}}" data-event-opts="{{[['tap',[['details',['$event']]]]]}}" bindtap="__e"><view class="left itemfolt"><image class="leftimg" src="/../kxsx_xcx/pages/my/mycollect/https:/kxsx-front-pic.oss-cn-beijing.aliyuncs.com/test/collectproduct-img.png"></image></view><view class="right itemfolt"><view class="title">{{item.title}}</view><view class="xaingqing">{{"详情："+item.detail}}</view><view class="shouting"><view class="shoutingdate">上次听到：<text class="date">{{item.date}}</text><text class="time">{{item.createtime}}</text></view><navigator class="goonlink">继续收听</navigator></view></view></view></block></view></view><view class="tabcont" hidden="{{currentId!=3}}"><view class="productlist"><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="productitem" data-p_id="{{item.p_id}}" data-event-opts="{{[['tap',[['goodsdetails',['$event']]]]]}}" bindtap="__e"><view class="leftitem leftitem_left"><image class="productimg" style="width:100%;height:100%;" src="{{item.image}}"></image></view><view class="leftitem leftitem_right"><view class="title">{{item.p_name}}</view><view class="jieshao"><text class="text-detail">{{item.p_detail}}</text></view><view class="collects" data-index="{{index}}" data-p_id="{{item.p_id}}" data-event-opts="{{[['tap',[['collectP',['$event']]]]]}}" catchtap="__e"><image class="collectimg_active" src="/static/img/freeTeaching/shoucangyi.png"></image><text class="collecttext_active">取消收藏</text></view><navigator class="seedetail">查看详情</navigator></view></view></block></view></view></view></view>