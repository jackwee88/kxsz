/* pages/catalogue/catalogue.wxss */
.catalogueList{
  margin-top: 40rpx;
  clear: both;
}
.catalogueImg{
  height: 32rpx;
  width: 32rpx;
  margin-right: 20rpx;
  margin-left: 18rpx;
}
.catalogueListTitle{
  margin-bottom: 20rpx;
}
.catalogueListTitle text{
  color: #3A821D;
  font-size: 36rpx;
  font-weight:500;
}
.catalogueContent{
  box-sizing: border-box;
  margin: 0 8rpx;
  clear: both;
}
.catalogueListPro{
  width: 180rpx;
  margin: auto;
}
.catalogueListImg{
  /* width: 180rpx; */
   height: 146rpx;
}
.catalogueContentList{
  float: left;
  width: calc(100% / 3);
  text-align: center;
  margin-bottom: 30rpx;
}
.catalogueContentListTitle{
  height: 48rpx;
  width: 180rpx;
  background: #3A821D;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 48rpx;
}
/*客服和返回顶部图标*/
.kefuFix{
  position: fixed;
  right: 6rpx;
  bottom: 320rpx;
  width: 100rpx;
  z-index: 999;
}
.kefuFix image{
  width: 56rpx;
  height: 56rpx;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.kefuFix view{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  background: #fff;
  box-shadow:0px 0px 8px 0px rgba(205,205,205,0.5);
  position: relative;
  margin-top: 16rpx;
}
.kefuFix button{
  padding: 0 ;
  border: 0;
  background: transparent;
}
.kefuFix button::after{
  border: 0;
}
/*客服和返回顶部图标结束*/
/*点击选择产品目录*/
.proFix{
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 9999;
}
.inputActivation{
  width: 580rpx;
	height: 410rpx;
	position: fixed;
	top: 0;
	right: 0;
	left: 0;
	bottom: 0;
	margin: auto;
	background: #FFFFFF;
	border-radius: 14rpx;
	overflow: hidden;
}
.inputActivationTop{
  height: 96rpx;
	background: #008800;
	position: relative;
	text-align: center;
}
.inputActivationQuxiao{
	float: right;
	color: #FFFFFF;
	width: 44rpx;
	height: 44rpx;
	display: inline-block;
	margin: 10rpx 10rpx 0 0;
	text-align: center;
	line-height: 44rpx;
	position: absolute;
	right: 10rpx;
	top: 10rpx;
  z-index: 999;
}
.inputActivationTopTitle{
	text-align: center;
	color: #FFFFFF;
	font-size: 32rpx;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  line-height:96rpx;
}
.inputActivationContent{
  margin: 0 38rpx;
}
.checkText{
  color: #666666;
  font-size: 32rpx;
  margin-top: 16rpx;
  text-align: center;
}
.checkNameOne{
  width: 240rpx;
  height: 80rpx;
  border: 1px solid #999999;
  text-align: center;
  line-height: 80rpx;
  display: inline-block;
  font-size: 32rpx;
  margin-top: 34rpx;
}
.checkNameOneNo{
  color: #666666;
  background: #fff;
}
.checkNameOneYes{
  color: #FFFFFF;
  background: #008800;
}
.checkYes{
  width: 268rpx;
	height: 72rpx;
	border-radius: 72rpx;
	font-size: 28rpx;
	text-align: center;
	line-height: 72rpx;
	margin-top: 46rpx;
}
.inputActivationBtnYes{
	background: #008800 !important;
	color: #FFFFFF !important;
}
.inputActivationBtnNo{
	background: #f7f7f7 !important;
	color: #999999 !important;
}
/*点击选择产品目录结束*/

