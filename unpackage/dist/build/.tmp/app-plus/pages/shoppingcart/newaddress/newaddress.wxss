/* pages/onlinestore/newaddress/newaddress.wxss */
page {
  background-color: #f5f5f5;
  box-sizing: border-box;
}
.form {
  width: 100%;
  background-color: #fff;
  padding: 0 30rpx;
  display: inline-block;
  border-top: 1rpx solid f3f3f3;
  margin-bottom: 20rpx;
}
.form_item {
  width: 92%;
  height: 90rpx;
  border-bottom: 1rpx solid #e5e5e5;
  position: relative;
  font-size: 32rpx;
  color: #666666;
}
.form_item:last-child {
  border-bottom: 0;
}
label {
  width: 140rpx;
  height: 90rpx;
  line-height: 90rpx;
  display: inline-block;
  text-align: justify;
}
.input {
  width: 80%;
  height: 90rpx;
  line-height: 90rpx;
  position: absolute;
  top: 0;
  left: 150rpx;
}
.area {
  min-height: 150rpx;
  position: relative;
  height: auto;
}
.righticon {
  width: 12rpx;
  height: 19rpx;
  background-image: url("data:image/png;base64,");
  background-size: 100% 100%;
  display: inline-block;
  position: absolute;
  right: 30rpx;
  top: 50rpx;
}
.detailaddress {
  width: 100%;
  position: relative;
  background-color: #fff;
}
.dizhiinput {
  height: 90rpx;
  line-height: 90rpx;
  position: absolute;
  top: 0;
  left: 150rpx;
}
.moren {
  height: 90rpx;
  line-height: 90rpx;
  padding: 0 30rpx;
  background-color: #fff;
  position: relative;
  margin-bottom: 60rpx;
}
.morentext {
  display: inline-block;
  width: 140rpx;
  font-size: 32rpx;
  color: #666666;
}
.rodio {
  position: absolute;
  top: -10rpx;
  right: 30rpx;
}
.saveaddress {
  width: 90%;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 10rpx;
  margin: 0 auto;
}
.savebtn {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  background-color: #48db8d;
  border-radius: 10rpx;
  color: #fff;
  border: none;
  font-size: 34rpx;
  text-align: center;
  outline: none;
}

